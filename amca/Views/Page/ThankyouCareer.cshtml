@{
    ViewBag.Title = "Thankyou";
    Layout = "~/Views/Shared/_ThankyouLayout.cshtml";
}
@model amca.Models.ExperienceDesignation
<div class="row">
    <div class="container">
        <div class="msgthankyou" style="width:auto">
            <h1>Thank You!</h1>
            <img src="~/wimages/check.svg" style="width: 10%" />
            <p><strong>Candidate Id: @TempData["Id"]</strong></p>
            <p>
                Dear <strong style="color:#333">@TempData["Name"]</strong>, Thanks for Applying for the position of <strong style="color:#333">@TempData["ApplyJob"]</strong>. We will review your Application and if finds suitable, will get back to you shortly.
            </p>
            <a href="@Url.Action("Index", "Page")">
                <button class="btn btnTheme">Go To Homepage</button>
            </a>
        </div>
    </div>
</div>

